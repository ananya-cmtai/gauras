{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "remote",
    "requireCommit": true
  },
   "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    
    },
    "preview": {
      "distribution": "internal",
      "android":{
        "buildType":"apk"
      },
      "channel":"preview"
    },
    "production": {
      "ios": {
    "simulator": false,
    "autoIncrement": true
  },
      "android": {
       "buildType": "app-bundle"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
